@page "{handler?}"
@model D301_WebApp.Pages.Dashboard.IndexModel

@section Styles {
    <link href="@Url.Content("~/css/dashboard.css")" rel="stylesheet" />
}

@{
    ViewData["Title"] = "Dashboard";
}

@if (User.Identity.IsAuthenticated)
{
    <div class="flex-dashboard">
        <div class="flex-column" style="flex-basis: 55%">
            <form method="post" asp-controller="Intakes">

                <div id="section-title">ADD YOUR INTAKES</div>
                <div class="flex-dashboard-dates">
                    <div class="container-date">
                        <div class="large-button-gradient" data-toggle="modal-intake" role="button" id="intake-btn" title="Add intake" data-placement="left" onmousemove="mouseover('intake-btn')"><span>+</span></div>

                        <a data-placement="top" title="Move back a day" asp-route-date=@Model.Date.AddDays(-1).ToString("yyyy-MM-dd") role="button" id="normalDateButton" style="float:left;" class="date-button tooltippy btn btn-secondary mb-1 large-button-gradient" onmousemove="mouseover('normalDateButton')"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-arrow-left"><line x1="19" y1="12" x2="5" y2="12"></line><polyline points="12 19 5 12 12 5"></polyline></svg></a>
                        <div class="btn-secondary" style="float:left;">@Model.Date.ToString("dd MMMM yyyy")</div>
                        @{
                            if (Model.Date >= DateTime.Now.Date)
                            {
                                <a role="button" class="btn btn-secondary mb-1 disabled button-gradient date-button" style="float:left; margin-right:10px;">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-arrow-right"><line x1="5" y1="12" x2="19" y2="12"></line><polyline points="12 5 19 12 12 19"></polyline></svg>
                                </a>
                            }
                            else
                            {
                                <a data-placement="top" title="Move forward a day" asp-route-date=@Model.Date.AddDays(1).ToString("yyyy-MM-dd") role="button" id="ifDateButton" class="date-button btn btn-secondary mb-1 large-button-gradient tooltippy" style="float:left; margin-right:10px;" onmousemove="mouseover('ifDateButton')">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-arrow-right"><line x1="5" y1="12" x2="19" y2="12"></line><polyline points="12 5 19 12 12 19"></polyline></svg>
                                </a>
                            }
                        }
                        @{
                            if (Model.Date == DateTime.Now.Date)
                            {
                                <div class="what-date">TODAY</div>
                            }
                            else
                            {

                                string modelDate = (Model.Date - DateTime.Now.Date).TotalDays.ToString().Trim('-');

                                if (modelDate == "1")
                                {
                                    <div class="what-date">@modelDate DAY AGO</div>
                                }
                                else
                                {
                                    <div class="what-date">@modelDate DAYS AGO</div>
                                }


                            }
                        }
                    </div>
                </div>


                @if (Model.IntakeList.Count() == 0)
                {
                    <div id="container-new" class="intake-item-tut">
                        <p>No intake for today!</p>
                    </div>
                }
                else
                {
            <div class="food-item-list-container">
